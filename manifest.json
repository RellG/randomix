{
  "manifest_version": 3,
  "name": "Randomix",
  "version": "2.3.0",
  "description": "Advanced random password generator with entropy-based strength meter",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Randomix - Password Generator"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
